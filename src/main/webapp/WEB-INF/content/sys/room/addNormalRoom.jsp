<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %><%@ page contentType="text/html;charset=UTF-8" language="java" %><html><head>    <%@ include file="../common/common.jsp" %></head><body><p style="font-size-adjust: inherit">普通房间管理</p><form class="form-horizontal" role="form" id="formId" action="${path}/sys/room/normal/save.do" method="post">    <div class="container-fluid">        <div class="row">            <div class="form-group">                <label class="col-md-4 control-label" style="font-size: larger">名称:</label>                <div class="col-md-3" style="text-align:left">                    <input  name="roomName"  type="text" class="form-control"                           placeholder="请输入名称"/>                </div>            </div>            <div class="form-group">                <label class="col-md-4 control-label" style="font-size: larger">房间个数:</label>                <div class="col-md-3" style="text-align:left">                    <input style="width:100%" name="roomNumber"  type="text"                           class="form-control"                           placeholder="请输入房间个数"/>                </div>            </div>            <div class="form-group">                <label class="col-md-4 control-label" style="font-size: larger">底分:</label>                <div class="col-md-3" style="text-align:left">                    <input style="width:100%" name="baseScore"  type="text"                           class="form-control"                           placeholder="请输入底分"/>                </div>            </div>            <div class="form-group">                <label class="col-md-4 control-label" style="font-size: larger">红包个数:</label>                <div class="col-md-3" style="text-align:left">                    <input style="width:100%" name="redPacketNumber"  type="text" class="form-control"                           placeholder="请输入红包个数"/>                </div>            </div>            <div class="form-group">                <label class="col-md-4 control-label" style="font-size: larger">倍数:</label>                <div class="col-md-3" style="text-align:left">                    <input style="width:100%" name="times"  type="text"                           class="form-control"                           placeholder="请输入倍数"/>                </div>            </div>            <div class="form-group">                <label class="col-md-4 control-label" style="font-size: larger">每个房间人数:</label>                <div class="col-md-3" style="text-align:left">                    <input style="width:100%" name="maxPlayer"  type="text"                           class="form-control"                           placeholder="请输入每个房间人数"/>                </div>            </div>        </div>        <br/>        <div class="form-horizontal">            <div class="col-md-3">                <button type="submit" class="btn btn-primary btn-sm" value="Sign up">提交</button>                &nbsp                <button type="button" id="resetBtn" class="btn btn-info btn-sm">重置</button>                &nbsp                <button type="button" class="btn btn-success btn-sm" onclick="back()">返回</button>            </div>        </div>    </div>    </div></form></body><script>    $(document).ready(function () {        /**         * 下面是进行插件初始化         * 你只需传入相应的键值对         * */        $('#formId').bootstrapValidator({            message: 'This value is not valid',            feedbackIcons: {                /*输入框不同状态，显示图片的样式*/                valid: 'glyphicon glyphicon-ok',                invalid: 'glyphicon glyphicon-remove',                validating: 'glyphicon glyphicon-refresh'            },            fields: {                /*验证*/                roomName: {                    /*键名name和input name值对应*/                    message: 'The name is not valid',                    validators: {                        notEmpty: {                            /*非空提示*/                            message: '名称不能为空'                        },                        stringLength: {                            /*长度提示*/                            min: 1,                            max: 20,                            message: '长度必须在1到20之间'                        }/*最后一个没有逗号*/                    }                },                roomNumber: {                    validators: {                        notEmpty: {                            message: '房间个数不能为空'                        },                        regexp: {                            regexp: /^[1-9]\d{0,2}$/,                            message: '整数且最大长度为3'                        }                    }                },                baseScore: {                    validators: {                        notEmpty: {                            message: '底分不能为空'                        },                        regexp: {                            regexp: /^[0-9]+([.]{1}[0-9]{0,2}){0,1}$/,                            message: '整数或者保留两位的小数'                        },                        stringLength: {                            /*长度提示*/                            min: 1,                            max: 6,                            message: '长度必须在1到6之间'                        }/*最后一个没有逗号*/                    }                },                redPacketNumber: {                    validators: {                        notEmpty: {                            message: '红包个数不能为空'                        },                        regexp: {                            regexp: /^[1-9]\d{0,2}$/,                            message: '整数且最大长度为3'                        }                    }                },                times: {                    validators: {                        notEmpty: {                            message: '倍数不能为空'                        },                        regexp: {                            regexp: /^[0-9]+([.]{1}[0-9]{0,2}){0,1}$/,                            message: '整数或者保留两位的小数'                        },                        stringLength: {                            /*长度提示*/                            min: 1,                            max: 5,                            message: '长度必须在1到5之间'                        }/*最后一个没有逗号*/                    }                },                maxPlayer: {                    validators: {                        notEmpty: {                            message: '每个房间人数不能为空'                        },                        regexp: {                            regexp: /^[1-9]\d{0,2}$/,                            message: '整数且最大长度为3'                        }                    }                }            }        });    });    function save() {        $('#formId').bootstrapValidator('validate');    }    $('#resetBtn').click(function () {        $('#formId').data('bootstrapValidator').resetForm(true);    });    function back() {        location.href = document.referrer;    }</script></html>